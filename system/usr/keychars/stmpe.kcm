}

key R {
    label:                              'R'
    base:                               'r'
    shift, capslock:                    'R'
    alt, lalt:                          '3'
    sym:                                '}'
}

key T {
    label:                              'T'
    base:                               't'
    shift, capslock:                    'T'
    alt, lalt:                          '('
    sym:                                '['
}

key Y {
    label:                              'Y'
    base:                               'y'
    shift, capslock:                    'Y'
    alt, lalt:                          ')'
