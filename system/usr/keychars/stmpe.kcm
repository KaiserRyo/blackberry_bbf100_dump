    alt, lalt:                          '$'
    sym:                                '$'
    lalt+lshift:                        '\uE141' # Special character means this is speaker key
}

### ROW 4

key 0 {
    label:                              '0'
    base:                               '0'
    shift, capslock:                    '0'
    alt, lalt:                          '0'
}

key SPACE {
    label:                              ' '
    base, shift, capslock, alt, lalt:   ' '
}

key ENTER {
    label:                              '\n'
    base:                               '\n'
}
