# Blackberry Physical Keyboard

type ALPHA

### ROW 1

key Q {
    label:                              'Q'
    base:                               'q'
    shift, capslock:                    'Q'
    alt, lalt:                          '#'
    sym:                                '~'
}

key W {
    label:                              'W'
    base:                               'w'
    shift, capslock:                    'W'
    alt, lalt:                          '1'
    sym:                                '`'
}

key E {
    label:                              'E'
